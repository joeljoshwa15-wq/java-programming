mport java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner JD= new Scanner(System.in);
        String name = JD.nextLine();
        int arrear = JD.nextInt();
        int cgpa = JD.nextInt();

        if (arrear == 1 && cgpa > 70)
        {
            System.out.println( "Name of the Student:" + name);
            System.out.println(name + " is Eligible for Placement");
        } else if ((arrear == 1 || arrear == 2) && cgpa > 75)
        {
            System.out.println( "Name of the Student:" + name);
            System.out.println(name + " is Eligible for Placement");
        } else
        {
            System.out.println( "Name of the Student:" + name);
            System.out.println(name + " is Not Eligible for Placement");

    }
}
}
