import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner JD = new Scanner(System.in);
        int a=JD.nextInt();
        int Joe= 0;
        for(int i=1;i<a;i++){
            if(a%i==0){
                Joe = Joe+i;
            }}
        if(Joe>a){
            System.out.println("Abundant Number");}
        else
           System.out.println("Not Abundant Number");} 
 
        
            
    }
