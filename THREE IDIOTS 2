import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        
        double num1= (a+c) / 2.0;
        double num2= (b+d) / 2.0;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",num1,num2);
    }
}
